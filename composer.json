{
    "name": "wearesho-team/yii2-streamtele",
    "description": "Streamtele integration",
    "type": "library",
    "license": "MIT",
    "require": {
        "guzzlehttp/guzzle": "^7.3",
        "horat1us/environment-config": "^1.4",
        "ext-json": "*",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.99",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Wearesho\\Streamtele\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wearesho\\Streamtele\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "./vendor/bin/phpcs --standard=PSR12 ./src ./tests",
        "phpcbf": "./vendor/bin/phpcbf --standard=PSR12 ./src ./tests",
        "phpstan": " vendor/bin/phpstan analyse src",
        "test": "./vendor/bin/phpunit"
    }
}
